<?xml version="1.0" encoding="UTF-8"?>
<infinispan
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="urn:infinispan:config:10.0"
  xsi:schemaLocation="urn:infinispan:config:10.0 http://www.infinispan.org/schemas/infinispan-config-10.0.xsd">
  <cache-container>
    <serialization marshaller="org.infinispan.commons.marshall.JavaSerializationMarshaller">
      <white-list>
        <class>org.jsr107.ri.annotations.DefaultGeneratedCacheKey</class>
        <regex>org.infinispan.example.*</regex>
      </white-list>
    </serialization>
  </cache-container>
</infinispan>