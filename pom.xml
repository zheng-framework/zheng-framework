<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://maven.apache.org/POM/4.0.0"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <artifactId>zheng</artifactId>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
        <groupId>org.apache.maven.plugins</groupId>
        <version>3.8.1</version>
      </plugin>
    </plugins>
  </build>
  <developers>
    <developer>
      <name>Zheng MingHai</name>
      <url>https://github.com/ysykzheng</url>
    </developer>
  </developers>

  <groupId>com.dadazhishi.zheng</groupId>
  <licenses>
    <license>
      <name>The Apache License, Version 2.0</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
    </license>
  </licenses>
  <modelVersion>4.0.0</modelVersion>
  <modules>
    <module>zheng-configuration</module>
    <module>zheng-cache</module>
    <module>zheng-cache-ehcache</module>
    <module>zheng-cache-cache2k</module>
    <module>zheng-cache-infinispan</module>
    <module>zheng-cache-caffeine</module>
    <module>zheng-cache-jcs</module>
    <module>zheng-configuration-ini</module>
    <module>zheng-configuration-toml</module>
    <module>zheng-bom</module>
    <module>zheng-web</module>
    <module>zheng-web-jetty</module>
    <module>zheng-web-undertow</module>
    <module>zheng-service</module>
    <module>zheng-rest-jersey</module>
    <module>zheng-rest-resteasy</module>
    <module>zheng-activemq</module>
    <module>zheng-activemq-artemis</module>
    <module>zheng-metrics</module>
    <module>zheng-swagger</module>
    <module>zheng-hibernate</module>
    <module>zheng-rabbitmq</module>
    <module>zheng-healthchecks</module>
    <module>zheng-mongodb</module>
    <module>zheng-memcached</module>
    <module>zheng-redis</module>
    <module>zheng-mybatis</module>
    <module>zheng-metrics-micrometer</module>
    <module>zheng-metrics-jersey</module>
    <module>zheng-metrics-servlet</module>
    <module>zheng-rest</module>
    <module>zheng-webjars</module>
  </modules>
  <packaging>pom</packaging>
  <properties>
    <maven.compiler.source>1.8</maven.compiler.source>
    <maven.compiler.target>1.8</maven.compiler.target>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
  <version>1.0</version>
</project>